@model WebApplication2.Models.DTO.HomeViewModel
<header id="header">
                    <div id="myCarousel" class="carousel slide carousel-content" style="height:550px;" data-ride="carousel">
                        <!-- Indicators -->
                        <ol class="carousel-indicators" style="z-index:900">
                            @if (Model.images != null && Model.images.Count() > 0)
                            {
                                for (int i = 0; i < Model.images.Count(); i++)
                                {
                                    <li data-target="#myCarousel" data-slide-to="@i"></li>
                                }
                            }
                            else
                            {
                                <li data-target="#myCarousel" data-slide-to="0"></li>
                            }

                        </ol>

                        <!-- Wrapper for slides -->
                        <div class="carousel-inner carousel-content" style="width:100%; height: 550px;">
                            @if (Model.images != null && Model.images.Count() > 0)
                            {
                                for (int i = 0; i < Model.images.Count(); i++)
                                {
                                    <div class="item @(i == 0 ? "active" : "") ">
                                        <img src="@Model.images[i].Path" style="width:100%; height:550px" class="carousel-content" alt="@Model.images[i].Name">

                                    </div>
                                }   
                            }
                            else
                            {
                                <div class="item active">
                                    <img src="~/Images/games/bannergame.jpg" alt="New York">
                                </div>

                            }

                        </div>

                        <!-- Left and right controls -->
                        <a class="left carousel-control" href="#myCarousel" data-slide="prev" style="z-index:900">
                            <span class="glyphicon glyphicon-chevron-left"></span>
                            <span class="sr-only">Previous</span>
                        </a>
                        <a class="right carousel-control" href="#myCarousel" data-slide="next" style="z-index:900">
                            <span class="glyphicon glyphicon-chevron-right"></span>
                            <span class="sr-only">Next</span>
                        </a>
                    </div>

                <div style="position:absolute;top:0px;width:100%;">
                    <div class="col-md-8 col-md-offset-2 intro-text" style="padding-top:350px;">
                        <h1 style="color:#@Model.content.loopcolor">Loop<span style="color: #@Model.content.tekcolor !important;"> Tek</span><span></span></h1>
                        @*#53bbc6*@ 
                        <br>
                        <!--  <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Duis sed dapibus leo nec ornare diam sed commodo nibh ante facilisis bibendum.</p> -->
                        <a href="#features" class="btn btn-custom btn-lg page-scroll">Learn More</a>
                    </div>
                </div>
</header>
<div id="about">
    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-md-6"> <img src="@Model.content.aboutImage" class="img-responsive" alt=""> </div>
            <div class="col-xs-12 col-md-6">
                <div class="about-text">
                    <h2>About Us</h2>
                    <p>@Model.content.AboutUs</p>
                    <div class="list-style">

                        <div class="col-lg-6 col-sm-6 col-xs-12">
                            <h3>MISSION</h3>

                            <p>
                                @Model.content.Mission
                            </p>
                        </div>
                        <div class="col-lg-6 col-sm-6 col-xs-12">
                            <h3>VISION</h3>

                            <p>
                                @Model.content.Vision
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="services" class="text-center">
    <div class="container">
        <div class="section-title">
            <h2>Our Services</h2>
        </div>
        <div class="row">
            @if (Model.services != null && Model.services.Count() > 0)
            {
                <center>
                    @foreach (var service in Model.services)
                    {
                        <div style="width:120px;margin:0px 20px;display:inline-block">
                            <a href="/projects/viewapps?catID=@service.ID"><img src="@service.IconLink" style="width:90px;height:90px;"></a>
                            <div class="service-desc">
                                <h6>@service.Name</h6>
                            </div>
                        </div>
                    }
                </center>
            }
            else
            {

                <div class="col-md-3">
                    <i class="fa fa-mobile"></i>
                    <div class="service-desc">
                        <h3>Mobile App</h3>
                    </div>
                </div>
                <div class="col-md-3">
                    <i class="fa fa-desktop"></i>
                    <div class="service-desc">
                        <h3>Desktop App</h3>
                    </div>
                </div>
                <div class="col-md-3">
                    <i class="fa fa-gamepad"></i>
                    <div class="service-desc">
                        <h3>Game App</h3>
                    </div>
                </div>
                <div class="col-md-3">
                    <i class="fa fa-gamepad"></i>
                    <div class="service-desc">
                        <h3>Web App</h3>
                    </div>
                </div>
            }

        </div>
    </div>
</div>
